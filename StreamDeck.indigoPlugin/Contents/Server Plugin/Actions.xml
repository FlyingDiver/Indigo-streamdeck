<?xml version="1.0"?>
<Actions>
	<SupportURL>http://forums.indigodomo.com/viewforum.php?f=213</SupportURL>
	<Action id="setButtonIcon">
        <Name>Set Button Icon</Name>
        <CallbackMethod>setButtonIcon</CallbackMethod>
        <ConfigUI>
            <Field id="device" type="menu">
                <Label>Stream Deck Device:</Label>
                <List class="self" filter="" method="availableDeckList"/>
            </Field>            
            <Field id="buttonIcon" type="textfield">
                <Label>Button Icon Path:</Label>
            </Field>
        </ConfigUI>
    </Action>
</Actions>
